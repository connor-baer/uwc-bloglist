{
  "name": "uwcblogs",
  "version": "6.0.0",
  "description":
    "A collection of over 390 blogs written by UWC students in 28 languages from 77 countries at the 17 United World Colleges.",
  "main": "app.js",
  "scripts": {
    "build": "NODE_ENV=production next build",
    "dev": "yarn prebuild && nodemon --watch server --inspect index.js",
    "deploy": "now",
    "fix": "yarn fix:prettier; yarn fix:eslint",
    "fix:eslint": "eslint --fix --quiet \"**/*.js\" \"**/*.js\"",
    "fix:prettier": "prettier --single-quote --write \"**/*.js\" \"**/*.js\"",
    "start": "NODE_ENV=production node index.js",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/connor-baer/uwcblogs.com.git"
  },
  "keywords": ["uwc", "blog", "blogs", "list"],
  "author": "Connor BÃ¤r, connor-baer <hello@connorbaer.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/connor-baer/uwcblogs.com/issues"
  },
  "homepage": "https://uwcblogs.com",
  "lint-staged": {
    "*.js": ["npm run fix:prettier", "git add"]
  },
  "now": {
    "alias": "uwcblogs.com",
    "env": {
      "NODE_ENV": "production"
    }
  },
  "dependencies": {
    "@std/esm": "^0.8.3",
    "akismet-api": "^3.0.0",
    "axios": "^0.16.2",
    "body-parser": "^1.18.0",
    "classnames": "^2.2.5",
    "contentful": "^4.6.2",
    "contentful-management": "^3.12.0",
    "express": "^4.16.2",
    "isomorphic-fetch": "^2.2.1",
    "ladda-cache": "^0.1.4",
    "ladda-logger": "^0.2.7",
    "lodash": "^4.17.4",
    "memory-cache": "^0.2.0",
    "moment": "^2.19.1",
    "morgan": "^1.9.0",
    "next": "^5.1.0",
    "next-routes": "^1.1.0",
    "nprogress": "^0.2.0",
    "open-color": "^1.4.0",
    "prop-types": "^15.5.10",
    "query-string": "^5.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-select": "^1.0.0-rc.10",
    "validator": "^9.0.0",
    "winston": "^2.4.0",
    "yargs": "^9.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-config-prettier": "^2.3.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.3.0",
    "husky": "^0.13.4",
    "lint-staged": "^3.6.0",
    "nodemon": "^1.12.1",
    "prettier": "^1.7.4"
  }
}
